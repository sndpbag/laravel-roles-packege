{
    "name": "sndpbag/laravel-dynamic-roles",
    "description": "Dynamic Role and Permission Management Package for Laravel",
    "keywords": ["laravel", "roles", "permissions", "rbac", "dynamic"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "sndpbag\\DynamicRoles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sndpbag\\DynamicRoles\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "sndpbag\\DynamicRoles\\Providers\\DynamicRolesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}